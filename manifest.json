{
  "name": "Auto Form Filler",
  "version": "0.1.0",
  "description": "Saves you time filling forms fields with dummy values while testing.",
  "permissions": ["tabs"],
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "128": "assets/icons/icon.png",
      "24": "assets/icons/icon.png",
      "32": "assets/icons/icon.png"
    },
    "default_title": "Auto Form Filler",
    "default_popup": "src/popup.html"
  },
  "icons": {
    "128": "assets/icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/js/contentScript.js"]
    }
  ],
  "author": "Jawad Shaikh"
}
